{
  "name": "vnas-toolbox",
  "version": "0.1.0",
  "description": "The Virtual Norwegian Toolbox",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64",
    "publish": "electron-builder --win --x64 -p always",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": "https://github.com/davwheat/VNAS-Toolbox",
  "keywords": [
    "Electron",
    "Norwegian",
    "discord",
    "vnas"
  ],
